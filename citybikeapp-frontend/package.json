{
  "name": "citybikeapp-frontend",
  "version": "0.0.1",
  "description": "City Bike App client",
  "scripts": {
    "build": "webpack --mode=production",
    "start": "webpack serve --mode=development",
    "lint": "eslint \"src/**/*\"",
    "client-gen": "openapi-generator-cli generate -i ../citybikeapp-backend/src/main/resources/api/api.yml -o src/generated -g typescript-axios --additional-properties=supportsES6=true",
    "preanalyze": "npm run build",
    "analyze": "source-map-explorer build/main.js --html sourcemap.html"
  },
  "author": "Matti Tuomikoski",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.5",
    "@types/react-datepicker": "^4.19.6",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "css-loader": "^6.11.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.36.1",
    "framer-motion": "^8.5.5",
    "html-webpack-plugin": "^5.6.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.5",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.5",
    "source-map-explorer": "^2.5.3"
  }
}
