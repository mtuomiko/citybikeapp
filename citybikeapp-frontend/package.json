{
  "name": "citybikeapp-frontend",
  "version": "0.0.1",
  "description": "City Bike App client",
  "scripts": {
    "build": "webpack --mode=production",
    "start": "webpack serve --mode=development",
    "lint": "eslint \"src/**/*\"",
    "client-gen": "openapi-generator-cli generate -i ../citybikeapp-backend/gen/api.yml -o src/generated -g typescript-axios --additional-properties=supportsES6=true",
    "preanalyze": "npm run build",
    "analyze": "source-map-explorer build/main.js --html sourcemap.html"
  },
  "author": "Matti Tuomikoski",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/icons": "^2.0.16",
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "css-loader": "^6.7.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "framer-motion": "^8.5.4",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "source-map-explorer": "^2.5.3"
  }
}
